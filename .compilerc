{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env"
          ],
          "react"
        ],
        "sourceMaps": "inline",
        "plugins": [
          "react-hot-loader/babel"
        ]
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env"
          ],
          "react"
        ],
        "sourceMaps": "none"
      }
    }
  }
}